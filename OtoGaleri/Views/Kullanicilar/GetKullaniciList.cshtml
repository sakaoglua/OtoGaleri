@using EntityLayer.Concrete
@model List<Kullanicilar>

@{
    ViewBag.Title = "GetKullaniciList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h2>Kullanıcı Sayfası</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Kullanıcı Adı</th>
        <th>Parola</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.K_Adi</td>
            <td>@item.K_Sifre</td>
        </tr>
    }
</table>
<br />
<a href="/Kullanicilar/AddKullanici" class="btn btn-info">Yeni Kullanıcı Ekle</a>